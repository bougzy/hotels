{
  "version": 2,
  "buildCommand": "cd client && npm install && npm run build",
  "installCommand": "npm install",
  "routes": [
    {
      "src": "/api/v1/(.*)",
      "dest": "/api/v1/[...path]"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/client/dist/index.html"
    }
  ]
}
